{
  "name": "MimeType Enforcer",
  "version": "1.0",
  "description": "Adds default mimetypes to .css and .js files",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/mime16.png",
      "32": "images/mime32.png",
      "48": "images/mime48.png",
      "128": "images/mime128.png"
    }
  },
  "icons": {
    "16": "images/mime16.png",
    "32": "images/mime32.png",
    "48": "images/mime48.png",
    "128": "images/mime128.png"
  },
  "permissions": [
    "declarativeWebRequest",
    "*://*/*"
  ],
  "author": "https://github.com/misterfresh"
}
